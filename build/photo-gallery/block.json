{
  "apiVersion": 2,
  "name": "photogallery/photo-gallery",
  "title": "PhotoBlocks Gallery",
  "version": "1.0.0",
  "textdomain": "photoblocks-gallery",
  "category": "media",
  "icon": "format-gallery",
  "description": "A powerful Gutenberg block for creating responsive",
  "attributes": {
    "images": {
      "type": "array",
      "default": []
    },
    "columns": {
      "type": "number",
      "default": 3
    },
    "gap": {
      "type": "number",
      "default": 16
    },
    "showCaptions": {
      "type": "boolean",
      "default": true
    },
    "imageSize": {
      "type": "string",
      "default": "full"
    },
    "customWidth": {
      "type": "number",
      "default": 0
    },
    "customHeight": {
      "type": "number",
      "default": 0
    },
    "layoutType": {
      "type": "string",
      "default": "grid"
    },
    "showPagination": {
      "type": "boolean",
      "default": false
    },
    "itemsPerPage": {
      "type": "number",
      "default": 12
    },
    "currentPage": {
      "type": "number",
      "default": 1
    },
    "align": {
      "type": "string",
      "default": "center"
    }
  },
  "supports": {
    "html": false,
    "align": [
      "left",
      "right",
      "full",
      "wide"
    ]
  },
  "editorScript": "file:./index.js",
  "editorStyle": "photo-gallery-editor",
  "style": "photo-gallery-style-frontend"
}